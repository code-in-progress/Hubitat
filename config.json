{
  "app": {
    "name": "Hubitat Elevation Logging Utility",
    "debug": true,
    "webserver": {
      "enable": true,
      "port": 80,
      "maker_api_url": "[YOUR MAKER API URL]"
    }
  },
  "hub": {
    "hub_host": "[YOUR HE IP/DNS NAME]",
    "events": {
      "enabled": true,
      "subscriptions": [
        41,
        423,
        425,
        418,
        419,
        420
      ],
      "socket_name": "eventsocket",
      "destinations": [
        {
          "type": "file",
          "enabled": true,
          "path": "he-events.log"
        },
        {
          "type": "csv",
          "enabled": true,
          "path": "he-events.csv",
          "delimiter": ",",
          "headers": [
            {
              "id": "source",
              "title": "source"
            },
            {
              "id": "name",
              "title": "name"
            },
            {
              "id": "displayName",
              "title": "displayName"
            },
            {
              "id": "value",
              "title": "value"
            },
            {
              "id": "unit",
              "title": "unit"
            },
            {
              "id": "deviceId",
              "title": "deviceId"
            },
            {
              "id": "hubId",
              "title": "hubId"
            },
            {
              "id": "locationId",
              "title": "locationId"
            },
            {
              "id": "installedAppId",
              "title": "installedAppId"
            },
            {
              "id": "descriptionText",
              "title": "descriptionText"
            },
            {
              "id": "date",
              "title": "date"
            },
            {
              "id": "timestamp",
              "title": "timestamp"
            }
          ]
        },
        {
          "type": "mysql",
          "enabled": true,
          "host": "[YOUR MYSQL SERVER]",
          "port": "3306",
          "user": "[MYSQL_USER]",
          "pass": "[MYSQL_PASS]",
          "db": "[DATABASE YOU CREATED]",
          "table": "events",
          "columns": [
            "source",
            "name",
            "displayName",
            "value",
            "unit",
            "deviceId",
            "hubId",
            "locationId",
            "installedAppId",
            "descriptionText"
          ],
          "customInsertStmt": null,
          "preSqlStatements": null,
          "postSqlStatements": null
        },
        {
          "type": "influxdb",
          "host": "localhost",
          "database": "express_response_db",
          "schema": [
            {
              "measurement": "response_times",
              "fields": {
                "path": "Influx.FieldType.STRING",
                "duration": "Influx.FieldType.INTEGER"
              },
              "tags": [
                "host"
              ]
            }
          ]
        },
        {
          "type": "console",
          "enabled": true
        }
      ]
    },
    "logs": {
      "enabled": true,
      "socket_name": "logsocket",
      "destinations": [
        {
          "type": "file",
          "enabled": true,
          "path": "he-logs.log"
        },
        {
          "type": "csv",
          "enabled": true,
          "path": "he-logs.csv",
          "delimiter": ",",
          "headers": [
            {
              "id": "name",
              "title": "name"
            },
            {
              "id": "msg",
              "title": "msg"
            },
            {
              "id": "id",
              "title": "id"
            },
            {
              "id": "time",
              "title": "time"
            },
            {
              "id": "type",
              "title": "type"
            },
            {
              "id": "level",
              "title": "level"
            }
          ]
        },
        {
          "type": "mysql",
          "enabled": true,
          "host": "[YOUR MYSQL SERVER]",
          "port": "3306",
          "user": "[MYSQL_USER]",
          "pass": "[MYSQL_PASS]",
          "db": "[DATABASE YOU CREATED]",
          "table": "logs",
          "columns": [
            "name",
            "msg",
            "id",
            "time",
            "type",
            "level"
          ],
          "customInsertStmt": null,
          "preSqlStatements": null,
          "postSqlStatements": null
        },
        {
          "type": "influxdb",
          "host": "localhost",
          "database": "express_response_db",
          "schema": [
            {
              "measurement": "response_times",
              "fields": {
                "path": "Influx.FieldType.STRING",
                "duration": "Influx.FieldType.INTEGER"
              },
              "tags": [
                "host"
              ]
            }
          ]
        },
        {
          "type": "console",
          "enabled": true
        }
      ]
    }
  }
}